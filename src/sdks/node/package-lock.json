{
  "name": "bvisor",
  "version": "0.0.3",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "bvisor",
      "version": "0.0.3",
      "workspaces": [
        "platforms/*"
      ],
      "dependencies": {
        "detect-libc": "^2.0.3"
      },
      "devDependencies": {
        "@types/node": "^25.2.0",
        "typescript": "^5.0.0"
      },
      "optionalDependencies": {
        "@bvisor/linux-arm64-gnu": "workspace:*",
        "@bvisor/linux-arm64-musl": "workspace:*",
        "@bvisor/linux-x64-gnu": "workspace:*",
        "@bvisor/linux-x64-musl": "workspace:*"
      }
    },
    "node_modules/@bvisor/linux-arm64-gnu": {
      "resolved": "platforms/linux-arm64-gnu",
      "link": true
    },
    "node_modules/@bvisor/linux-arm64-musl": {
      "resolved": "platforms/linux-arm64-musl",
      "link": true
    },
    "node_modules/@bvisor/linux-x64-gnu": {
      "resolved": "platforms/linux-x64-gnu",
      "link": true
    },
    "node_modules/@bvisor/linux-x64-musl": {
      "resolved": "platforms/linux-x64-musl",
      "link": true
    },
    "node_modules/@types/node": {
      "version": "25.2.0",
      "dev": true,
      "license": "MIT",
      "dependencies": {
        "undici-types": "~7.16.0"
      }
    },
    "node_modules/detect-libc": {
      "version": "2.1.2",
      "license": "Apache-2.0",
      "engines": {
        "node": ">=8"
      }
    },
    "node_modules/typescript": {
      "version": "5.9.3",
      "dev": true,
      "license": "Apache-2.0",
      "bin": {
        "tsc": "bin/tsc",
        "tsserver": "bin/tsserver"
      },
      "engines": {
        "node": ">=14.17"
      }
    },
    "node_modules/undici-types": {
      "version": "7.16.0",
      "dev": true,
      "license": "MIT"
    },
    "platforms/linux-arm64-gnu": {
      "name": "@bvisor/linux-arm64-gnu",
      "version": "0.0.3",
      "cpu": [
        "arm64"
      ],
      "os": [
        "linux"
      ]
    },
    "platforms/linux-arm64-musl": {
      "name": "@bvisor/linux-arm64-musl",
      "version": "0.0.3",
      "cpu": [
        "arm64"
      ],
      "os": [
        "linux"
      ]
    },
    "platforms/linux-x64-gnu": {
      "name": "@bvisor/linux-x64-gnu",
      "version": "0.0.3",
      "cpu": [
        "x64"
      ],
      "os": [
        "linux"
      ]
    },
    "platforms/linux-x64-musl": {
      "name": "@bvisor/linux-x64-musl",
      "version": "0.0.3",
      "cpu": [
        "x64"
      ],
      "os": [
        "linux"
      ]
    }
  }
}
